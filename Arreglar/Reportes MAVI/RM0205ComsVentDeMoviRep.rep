TipoTabla=General
SQL=declare <BR>         @iva float<BR><BR>         set @iva=(select ((defimpuesto)/100 )as ivaact from empresagral where empresa=<T>MAVI<T>)<BR><BR><BR><BR>   SELECT <TAB><BR>        IVA=@iva,<BR><TAB>SUC=VN.SUCURSALVENTA,<BR><TAB>MOV=VN.MOV,<BR><TAB>FACTURA=VN.MOVID,<BR><TAB>CONCEPTO=VN.CONCEPTO,<BR><TAB>FechaMavi=VN.FechaMavi,<BR><TAB>CLIENTE=VN.CLIENTE,<BR><TAB>NOMBRE=CTE.NOMBRE,<BR><TAB>TELEFONOS=CTE.TELEFONOS,<BR><TAB>ARTICULO=VN.ARTICULO,<BR><TAB>DESCRIPCION=ART.DESCRIPCION1,<BR><TAB>CANTIDAD=VN.CANTIDAD,<BR><TAB>PRECIO=VN.PRECIO,<BR><TAB>IMPORTE=CASE <BR><TAB><TAB>WHEN VN.MOV=<T>FACTURA<T> THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))<BR><TAB><TAB>WHEN VN.MOV=<T>DEVOLUCION VENTA<T> THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))*(-1)<BR><TAB><TAB>WHEN VN.MOV=<T>NOTA CREDITO<T> <CONTINUA>
SQL002=<CONTINUA>THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))*(-1)<BR><TAB><TAB>WHEN VN.MOV=<T>FACTURA VIU<T> THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))<BR><TAB><TAB>WHEN VN.MOV=<T>DEVOLUCION VIU<T> THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))*(-1)<BR><TAB><TAB>WHEN VN.MOV=<T>NOTA CREDITO VIU<T> THEN (VN.CANTIDAD*(VN.PRECIO-VN.DESCUENTOIMPORTE))*(-1)<BR><TAB><TAB>END <BR><TAB>,VN.DESCUENTOIMPORTE,<BR><TAB>ORIGEN=VN.ORIGEN,<BR><TAB>ORIGENID=VN.ORIGENID,<BR><TAB>VN.UEN,<BR><TAB>ORDEN=CASE<BR><TAB>WHEN VN.MOV=<T>FACTURA<T> THEN 1<BR><TAB>WHEN VN.MOV=<T>DEVOLUCION VENTA<T> THEN 2<BR><TAB>WHEN VN.MOV=<T>NOTA CREDITO<T> THEN 3<BR><TAB>WHEN VN.MOV=<T>FACTURA VIU<T> THEN 4<BR><TAB>WHEN VN.MOV=<T>DEVOLUCION VIU<T> THEN 5<BR><TAB>WHEN VN.MOV=<T>NOTA CREDITO VIU<T> THEN 6<BR><TAB>END <BR><TAB>FROM<CONTINUA>
SQL003=<CONTINUA> V_MAVIVENTASNETAS VN<BR><TAB>LEFT JOIN VENTASCANALMAVI VCM ON VN.ENVIARA=VCM.ID<BR><TAB>INNER JOIN CTE CTE ON VN.CLIENTE=CTE.CLIENTE<BR><TAB>INNER JOIN ART ON VN.ARTICULO=ART.ARTICULO<BR><TAB>WHERE <BR><TAB>VN.MOV IN (<T>FACTURA<T>,<T>DEVOLUCION VENTA<T>,<T>NOTA CREDITO<T>,<T>FACTURA VIU<T>,<T>DEVOLUCION VIU<T>,<T>NOTA CREDITO VIU<T>)<BR><TAB><TAB><TAB> AND VN.ORIGEN IN (<T>FACTURA<T>,<T>FACTURA VIU<T>)<BR><TAB><TAB><TAB><TAB><TAB> AND VN.CATEGORIA=<T>VENTA<T><BR><TAB>GROUP BY VN.SUCURSALVENTA,VCM.CATEGORIA,VN.ID,VN.CATEGORIA, VN.GRUPO, VN.FAMILIA ,VN.LINEA, VN.ENVIARA,VN.MOV,VN.MOVID,VN.CONCEPTO,VN.FechaMavi,VN.CLIENTE,CTE.NOMBRE,CTE.TELEFONOS,VN.ARTICULO,ART.DESCRIPCION1,VN.CANTIDAD,VN.PRECIO,VN.DESCUENTOIMPORTE,VN.CANTIDAD,VN.ID,VN.ORIGEN,VN.ORIGENID,VN.UEN<BR><TAB>ORDER BY UEN, ORDEN,<CONTINUA>
