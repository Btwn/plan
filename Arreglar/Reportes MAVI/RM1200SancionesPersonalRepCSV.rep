SQL={si <BR>condatos(Mavi.RM1200Plazas)<BR>Entonces <BR><T><BR>SELECT<BR>  e.Nombre,<BR>  e.Nomina,<BR>  e.Puesto,<BR>  e.Departamento,<BR>  SUM(e.Sancion) as Sancion,<BR>  p.descripcion,   <BR>  Categoria=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>CATEGOR페<T>)+<T>), <BR>  Subcategoria=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>SUBCATEGOR페<T>)+<T>), <BR>  Quincenas=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>QUINCENAS<T>)+<T>),<BR>  PorDescontar=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>POR DESCONTAR<T>)+<T>),<BR>  Fecha =Cast(<T>+Comillas(FechaFormatoServidor(Info.FechaD))+<T> As Varchar) +<T>+Comillas(<T>-<T>)+<T>+ Cast(<T>+Comillas(FechaFormatoServidor(Info.FechaA))+<T> As Varchar) <BR>FROM credidSancionPersonal e<BR>INNER JOIN comercializadora.dbo.plaza p<BR>  ON e.puesto = p.puesto<BR>  AND e.departamento = p.departamento<BR>  AND e.plaza = p.plaza<BR>  AND p.descripcion IN (<T>+Mavi.RM1200Plazas+<T>)<BR>  AND FechaSancion between <T> + Comillas(FechaFormatoServidor(Info.FechaD) + <T> 00:00:00<T>)+ <T> and <T> + Comillas(FechaFormatoServidor(Info.FechaA)+ <T> 00:00:00<T>)+<T><BR>GROUP BY e.Nomina,<BR>         e.Nombre,<BR>         e.Puesto,<BR>         e.Departamento,<BR>         p.descripcion<T><TAB><TAB> <BR>Sino<BR><BR><T>SELECT<BR>  e.Nombre,<BR>  e.Nomina,<BR>  e.Puesto,<BR>  e.Departamento,<BR>  SUM(e.Sancion) as Sancion,<BR>  p.descripcion, <BR>  Categoria=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>CATEGOR페<T>)+<T>), <BR>  Subcategoria=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>SUBCATEGOR페<T>)+<T>), <BR>  Quincenas=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>QUINCENAS<T>)+<T>),<BR>  PorDescontar=(Select VALOR from TablaStD where TablaSt =<T>+Comillas(<T>RM1200 DATOS EXPORTACI흁 CSV<T>)+<T> AND Nombre=<T>+Comillas(<T>POR DESCONTAR<T>)+<T>),<BR>  Fecha =Cast(<T>+Comillas(FechaFormatoServidor(Info.FechaD))+<T> As Varchar) +<T>+Comillas(<T>-<T>)+<T>+ Cast(<T>+Comillas(FechaFormatoServidor(Info.FechaA))+<T> As Varchar) <BR>FROM credidSancionPersonal e<BR>INNER JOIN comercializadora.dbo.plaza p<BR>  ON e.puesto = p.puesto<BR>  AND e.departamento = p.departamento<BR>  AND e.plaza = p.plaza<BR>  AND FechaSancion between <T> + Comillas(FechaFormatoServidor(Info.FechaD) + <T> 00:00:00<T>) + <T> and <T>+ Comillas(FechaFormatoServidor(Info.FechaA) + <T> 00:00:00<T>) + <T><BR>GROUP BY e.Nomina,<BR>         e.Nombre,<BR>         e.Puesto,<BR>         e.departamento,<BR>         p.descripcion<T><BR>Fin}
