SQL=DECLARE @fechad nvarchar(max),<BR><TAB><TAB>@fechaa nvarchar(max)    <BR>SET @fechad = <T>{Fechaformatoservidor(Info.FechaD)}<T>  <BR>SET @fechaa = <T>{Fechaformatoservidor(Info.FechaA)}<T>  <BR>  <BR>SELECT * FROM (                  <BR>SELECT Tienda=S.Sucursal,Grupo = S.grupo,Categoria = S.categoria,Tipo = S.tipo,<BR><TAB>AgrupadorMtro = T.GMTRO,CategoriaVta = t.Categoria,<TAB><TAB><BR><TAB>TC12M = T.tc12,<BR><TAB>TC18M = T.tc18,<BR><TAB>Contado = T.conta,<BR><TAB>Fonacot = T.fona,<BR><TAB>Fide = T.fid,<BR><TAB>CreditoSoloCelC = T.YcelularC,<BR><TAB>CreditoSoloCelN = T.YcelularN,<BR><TAB>CreditoSinCelC = T.NcelularC,<BR><TAB>CreditoSinCelN = T.NcelularN,<BR><TAB>Instituciones = T.institu,<BR><TAB>Muebles = T.mueble,<BR><TAB>PrestamosCapital = T.capital,<BR><TAB>PrestamosFinan = T.financi<CONTINUA>
