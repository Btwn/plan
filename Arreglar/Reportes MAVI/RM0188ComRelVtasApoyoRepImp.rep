SQL=SELECT <TAB>UEN,MOV,MOVID,ORIGEN,ORIGENID,PROVEEDOR,Replace(Replace(Replace(Replace(Replace(Replace(Replace(NOMBRE, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>),<T>´<T>,<T>-<T>) AS NOMBRE,TIENDA,NOMSUCU,Replace(Replace(Replace(Replace(Replace(Replace(CLIENTE, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS CLIENTE,ENVIARA,FECHAEMISION,Replace(Replace(Replace(Replace(Replace(Replace(ARTICULO, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS ARTICULO,CONCEPTO,CANTIDAD = CASE WHEN Len(SerieLote) > 0 AND (MOV LIKE <T>Cancela%<T> OR MOV LIKE <T>Devolu%<T>) THEN -1 WHEN Len(SerieLote) > 0 AND (MOV NOT LIKE <T>Cancela%<T> OR MOV NOT LIKE <T>Devolu%<T>) THEN 1 ELSE SUM(CANTIDAD) END,<TAB>ESTATUS,Replace(Replace(Replace(Replace(Replace(Replace(Replace(DESCRIPCION1, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>),<T>¥<T>,<T>N<T>) AS DESCRIPCION1,CostoFinal,Costo,Precio,Categoria,ID,Agente,IVAACT,RenglonTipo,ArtCategoria,Replace(Replace(Replace(Replace(Replace(Replace(ArtGrupo, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS ArtGrupo,Replace(Replace(Replace(Replace(Replace(Replace(ArtFamilia, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS ArtFamilia,Replace(Replace(Replace(Replace(Replace(Replace(ArtLinea, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS ArtLinea,SerieLote = CASE WHEN Len(SerieLote) > 0 THEN (<T><T><T><T>+SerieLote+<T><T><T><T>) ELSE SerieLote END,PDesc,XFecha,Replace(Replace(Replace(Replace(Replace(Replace(NombreCte, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS NombreCte,Replace(Replace(Replace(Replace(Replace(Replace(Autoriza, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS Autoriza,Replace(Replace(Replace(Replace(Replace(Replace(UsuarioDescuento, <T>Á<T>, <T>A<T>), <T>É<T>, <T>E<T>), <T>Í<T>, <T>I<T>),<T>Ó<T>,<T>O<T>),<T>Ú<T>,<T>U<T>),<T>Ñ<T>,<T>N<T>) AS UsuarioDescuento,Normal,AgenteServicio,Replace(Replace(Replace(Replace(Replace(Replace(FormaCobro, <T>á<T>, <T>a<T>), <T>é<T>, <T>e<T>), <T>í<T>, <T>i<T>),<T>ó<T>,<T><T>),<T>ú<T>,<T>u<T>),<T>ñ<T>,<T>n<T>) AS FormaCobro<TAB><TAB><TAB><TAB><TAB><TAB><BR>FROM ( <BR><TAB> Select V.UEN,V.MOV,V.MOVID,A.PROVEEDOR,P.NOMBRE,TIENDA=V.SucursalVenta,NOMSUCU=S.NOMBRE,V.CLIENTE,V.ENVIARA,FECHAEMISION=DATEADD(DD,0,DATEDIFF(DD,0,V.FechaMavi)),<BR><TAB><TAB>VD.ARTICULO,CONCEPTO = V.CONDICION,VD.CANTIDAD,V.ESTATUS,A.DESCRIPCION1,PM.CostoFinal,Precio=VD.Precio/(1+(VD.Impuesto1/100)),VCM.Categoria,VCM.ID,<BR><TAB><TAB>Agente=V.AgteVendedor,IVAACT=(SELECT DEFIMPUESTO FROM EmpresaGral WHERE Empresa=<T>MAVI<T>) * .01,Origen=V.Origen,OrigenID=V.OrigenID,<BR><TAB><TAB>PArt.Costo,VD.RenglonTipo,ArtCategoria = A.Categoria,ArtGrupo = A.Grupo,ArtFamilia = A.Familia,ArtLinea = A.Linea,SL.SerieLote,<BR><TAB><TAB>PDesc = CASE WHEN V.Mov In (<T>Factura<T>,<T>Factura VIU<T>,<T>Factura Mayoreo<T>) THEN Round((((VD.PrecioAnterior - VD.Precio)*100)/VD.Precio),2) Else 0<TAB><TAB><TAB><TAB><TAB><TAB><TAB><BR><TAB><TAB><TAB><TAB><TAB><TAB>END, <BR><TAB><TAB>XFecha = CASE WHEN VCM.Categoria In (<T>CONTADO<T>,<T>CREDITO EXTERNO<T>) THEN CONVERT(datetime,CONVERT(varchar(10),V.FechaMavi,101),101)<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB>  WHEN VCM.Categoria In (<T>CREDITO MENUDEO<T>,<T>ASOCIADOS<T>,<T>MAYOREO<T>,<T>INSTITUCIONES<T>) THEN VA.FechaEmision  <BR><TAB><TAB><TAB><TAB><TAB><TAB> END,<BR><TAB><TAB>NombreCte=C.Nombre,autoriza=AUD.UsuarioAutorizo+<T> <T>+Aut.Nombre, UsuarioDescuento=U.Usuario+<T> <T>+U.Nombre, Normal=Case When VD.PrecioAnterior IS null Then VD.Precio Else VD.PrecioAnterior End, V.AgenteServicio,V.FormaCobro<BR><TAB> FROM(<BR><TAB><TAB>SELECT DISTINCT  V.Id,V.Mov,V.Cliente,V.SucursalVenta,V.EnviarA,V.Estatus,V.Uen,V.MovId,V.FechaMavi,V.Condicion,<BR><TAB><TAB><TAB>V.AgteVendedor,Venta.Origen,Venta.OrigenID, Venta.AgenteServicio,Venta.FormaCobro<BR><TAB><TAB>FROM V_MaviVentasNetas V WITH(NOLOCK)<BR><TAB><TAB>INNER JOIN MovTipo MT WITH(NOLOCK) ON MT.Mov=V.Mov AND MT.Modulo=<T>VTAS<T> AND MT.Clave=<T>VTAS.F<T><BR><TAB><TAB>INNER JOIN Venta WITH(NOLOCK) ON Venta.Mov=V.Mov AND Venta.MovID=V.MovId AND V.Mov like <T>Fac%<T><BR><TAB><TAB>) V <BR><TAB><TAB>LEFT JOIN VENTAD VD WITH(NOLOCK) ON VD.ID = V.ID<BR><TAB><TAB>LEFT JOIN PropreArt PArt ON Part.Art = VD.Articulo<BR><TAB><TAB>LEFT JOIN ART A WITH(NOLOCK) ON A.ARTICULO = VD.ARTICULO<BR><TAB><TAB>LEFT JOIN CTE C WITH(NOLOCK) ON C.CLIENTE = V.CLIENTE<BR><TAB><TAB>LEFT JOIN PROV P WITH(NOLOCK) ON P.PROVEEDOR = A.PROVEEDOR<BR><TAB><TAB>LEFT JOIN SUCURSAL S WITH(NOLOCK) ON S.SUCURSAL = V.SucursalVenta<BR><TAB><TAB>LEFT JOIN ventascanalmavi VCM WITH(NOLOCK) ON  VCM.ID = V.ENVIARA<BR><TAB><TAB>LEFT JOIN PlaneadorMacroHistMAVI PM WITH(NOLOCK) on VD.Articulo = PM.Codigo And PM.Proveedor = A.Proveedor<BR><TAB><TAB>LEFT JOIN SerieLoteMov SL WITH(NOLOCK) on sl.Id = vd.id and vd.renglonid = sl.renglonid AND SL.Modulo=<T>VTAS<T> AND Vd.Articulo=Sl.Articulo<BR><TAB><TAB>LEFT JOIN Venta VA WITH(NOLOCK) ON V.Origen=VA.Mov AND V.OrigenID=VA.MovID<BR><TAB><TAB>LEFT JOIN Venta VS With(NOLOCK) ON VA.Origen=VS.Mov And VA.OrigenID=VS.MovID<BR><TAB><TAB>LEFT JOIN Venta VF With(NoLock) ON VS.Origen=VF.Mov and VS.OrigenID=VF.MovID<BR><TAB><TAB>LEFT JOIN Cte ON Cte.Cliente=V.Cliente<BR><TAB><TAB>LEFT JOIN Usuario U WITH(NOLOCK) ON U.Usuario=VD.UsuarioDescuento<BR><TAB><TAB>LEFT JOIN AutorizacionesUsuarioDescuentoMavi AUD With(NoLock) ON AUD.VtaID=VF.ID And AUD.VtaRenglon=VD.Renglon<BR><TAB><TAB>LEFT JOIN Usuario Aut With(NoLock) ON Aut.Usuario=AUD.UsuarioAutorizo<BR><TAB>WHERE ((V.Mov like <T>Fac%<T> AND V.Estatus=<T>CONCLUIDO<T>) )<BR><TAB><TAB>{SI(ConDatos(Info.UEN),<T> AND V.UEN=<T>+Info.UEN,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.ProveedorD),<T> AND A.PROVEEDOR =<T>+Comillas(Info.ProveedorD),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188Condicion),<T> And V.CONDICION in(<T>+Mavi.RM0188Condicion+<T>)<T>,<T><T>)}<BR><TAB><TAB>AND DATEADD(DD,0,DATEDIFF(DD,0,V.FechaMavi)) Between <T>{FechaFormatoServidor(Info.FechaD)}<T> AND <T>{FechaFormatoServidor(Info.FechaA)}<T><BR><TAB><TAB>{Si(Condatos(Mavi.RM0188TipoMovimiento),<T> And V.MOV in(<T>+Mavi.RM0188TipoMovimiento+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CanalVtaCatxUEN),<T> And VCM.ID in(<T>+Mavi.RM0188CanalVtaCatxUEN+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CatCanal),<T> And VCM.Categoria =<T>+Comillas(Mavi.RM0188CatCanal),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtCatLigGrup),<T> AND A.Categoria=<T>+Comillas(Mavi.RM0188ArtCatLigGrup),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtGrupLigFam),<T> AND A.Grupo=<T>+Comillas(Mavi.RM0188ArtGrupLigFam),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtFamLigLin),<T> And A.Familia in(<T>+Mavi.RM0188ArtFamLigLin+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtLinLigLin),<T> And A.Linea in(<T>+Mavi.RM0188ArtLinLigLin+<T>)<T>,<T><T>)}<BR><BR><TAB>UNION ALL<BR><BR><TAB> Select Distinct V.UEN,V.MOV,V.MOVID,A.PROVEEDOR,P.NOMBRE,TIENDA=V.SucursalVenta,NOMSUCU=S.NOMBRE,V.CLIENTE,V.ENVIARA,FECHAEMISION=DATEADD(DD,0,DATEDIFF(DD,0,V.FECHAEMISION)),<BR><TAB><TAB>VD.ARTICULO,CONCEPTO = V.CONDICION,VD.CANTIDAD,V.ESTATUS,A.DESCRIPCION1,PM.CostoFinal,Precio=VD.Precio/(1+(VD.Impuesto1/100)),VCM.Categoria,VCM.ID,<BR><TAB><TAB>V.Agente,IVAACT=(SELECT DEFIMPUESTO FROM EmpresaGral WHERE Empresa=<T>MAVI<T>) * .01,Origen=V.Origen,OrigenID=V.OrigenID,PArt.Costo,VD.RenglonTipo,<BR><TAB><TAB>ArtCategoria = A.Categoria,ArtGrupo = A.Grupo,ArtFamilia = A.Familia,ArtLinea = A.Linea,sl.SerieLote,<BR><TAB><TAB>PDesc = CASE WHEN V.Mov In (<T>Solicitud Credito<T>,<T>Pedido<T>,<T>Solicitud Mayoreo<T>,<T>Pedido Mayoreo<T>) /*AND A.FAMILIA<><T>VARIOS FINANCIEROS<T>*/ THEN Round((((VD.PrecioAnterior - VD.Precio)*100)/VD.Precio),2) <TAB><TAB><TAB><TAB> <BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB> ELSE 0 <BR><TAB><TAB><TAB><TAB><TAB><TAB>END, <BR><TAB>    XFecha = CASE WHEN V.Mov IN (<T>Solicitud Credito<T>,<T>Solicitud Mayoreo<T>)  Then Convert(Date, V.FechaEmision,103)  <BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB>  WHEN V.Mov IN (<T>Pedido<T>,<T>Pedido Mayoreo<T>)  AND VCM.Categoria In (<T>CONTADO<T>,<T>CREDITO EXTERNO<T>) THEN  V.FechaEmision<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB>  Else Convert(Date,VS.FechaEmision,103)  <BR><TAB><TAB><TAB><TAB><TAB><TAB> END,<BR><TAB><TAB>NombreCte=C.Nombre,autoriza=Case when V.Mov in (<T>Pedido<T>, <T>Pedido Mayoreo<T>) Then  Aut.Usuario+<T> <T>+Aut.Nombre<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB> When V.Mov In (<T>Solicitud Credito<T>,<T>Solicitud Mayoreo<T>) Then Aut1.Usuario+<T> <T>+Aut1.Nombre <BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB> When V.Mov In (<T>ANALISIS CREDITO<T>,<T>ANALISIS MAYOREO<T>) Then Aut2.Usuario+<T> <T>+Aut2.Nombre  Else <T> <T>End, <BR><TAB><TAB>UsuarioDescuento=U.Usuario+<T> <T>+U.Nombre, Normal=Case When VD.PrecioAnterior IS null Then VD.Precio Else VD.PrecioAnterior End, V.AgenteServicio,V.FormaCobro<TAB><TAB><TAB> <TAB><TAB><TAB><TAB><TAB><TAB><BR><TAB> FROM<BR><TAB><TAB>VENTA V WITH(NOLOCK)<BR><TAB><TAB>LEFT JOIN VENTAD VD WITH(NOLOCK) ON VD.ID = V.ID<BR><TAB><TAB>LEFT JOIN PropreArt PArt ON Part.Art = VD.Articulo<BR><TAB><TAB>LEFT JOIN ART A WITH(NOLOCK) ON A.ARTICULO = VD.ARTICULO<BR><TAB><TAB>LEFT JOIN CTE C WITH(NOLOCK) ON C.CLIENTE = V.CLIENTE<BR><TAB><TAB>LEFT JOIN PROV P WITH(NOLOCK) ON P.PROVEEDOR = A.PROVEEDOR<BR><TAB><TAB>LEFT JOIN SUCURSAL S WITH(NOLOCK) ON S.SUCURSAL = V.SucursalVenta<BR><TAB><TAB>LEFT JOIN ventascanalmavi VCM WITH(NOLOCK) ON  VCM.ID = V.ENVIARA<BR><TAB><TAB>LEFT JOIN PlaneadorMacroHistMAVI PM WITH(NOLOCK) on VD.Articulo = PM.Codigo And PM.Proveedor = A.Proveedor<BR><TAB><TAB>LEFT JOIN SerieLoteMov SL WITH(NOLOCK) on sl.Id = vd.id and vd.renglonid = sl.renglonid AND SL.Modulo=<T>VTAS<T> AND Vd.Articulo=Sl.Articulo<BR><TAB><TAB>LEFT JOIN Usuario U WITH(NOLOCK) ON U.Usuario=VD.UsuarioDescuento <BR><TAB><TAB>LEFT JOIN Venta VS WITH(NOLOCK) ON VS.Mov=V.Origen AND VS.MovID=V.OrigenID<BR><TAB><TAB>LEFT JOIN Venta VF WITH(NOLOCK) ON VF.Mov=VS.Origen AND VS.OrigenID=VF.MovID<BR><TAB><TAB>LEFT JOIN Venta VA With(NoLock) ON VA.Mov=V.Origen and VA.MovID=V.OrigenID and V.Mov in (<T>ANALISIS CREDITO<T>,<T>ANALISIS MAYOREO<T>)<BR><TAB><TAB>LEFT JOIN AutorizacionesUsuarioDescuentoMavi AUD ON AUD.VtaID=VF.ID  and AUD.VtaRenglon=VD.Renglon<BR><TAB><TAB>LEFT JOIN AutorizacionesUsuarioDescuentoMavi AUD1 ON AUD1.VtaID=V.ID And AUD1.VtaRenglon=VD.Renglon And V.Mov in (<T>Solicitud Credito<T>,<T>Solicitud Mayoreo<T>)<BR><TAB><TAB>LEFT Join AutorizacionesUsuarioDescuentoMavi AUD2 ON AUD2.VtaID=VA.ID and AUD2.VtaRenglon=VD.Renglon<BR><TAB><TAB>LEFT JOIN Usuario Aut With(NoLock) ON Aut.Usuario=AUD.UsuarioAutorizo<BR><TAB><TAB>LEFT JOIN Usuario Aut1 With(NoLock) ON Aut1.Usuario=AUD1.UsuarioAutorizo  <BR><TAB><TAB>LEFT JOIN Usuario Aut2 With(NoLock) ON Aut2.Usuario=AUD2.UsuarioAutorizo <BR><TAB>WHERE (V.MOV IN (<T>SOLICITUD CREDITO<T>,<T>SOLICITUD MAYOREO<T>,<T>PEDIDO<T>,<T>PEDIDO MAYOREO<T>,<T>ANALISIS CREDITO<T>,<T>ANALISIS MAYOREO<T>) <BR><TAB><TAB>AND V.ESTATUS=<T>PENDIENTE<T> AND VD.Unidad<><T>SERVICIO<T>) <BR><TAB><TAB>{SI(ConDatos(Info.UEN),<T> AND V.UEN=<T>+Info.UEN,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.ProveedorD),<T> AND A.PROVEEDOR =<T>+Comillas(Info.ProveedorD),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188Condicion),<T> And V.CONDICION in(<T>+Mavi.RM0188Condicion+<T>)<T>,<T><T>)}<BR><TAB><TAB>AND DATEADD(DD,0,DATEDIFF(DD,0,V.FECHAEMISION)) Between <T>{FechaFormatoServidor(Info.FechaD)}<T> AND <T>{FechaFormatoServidor(Info.FechaA)}<T><BR><TAB><TAB>{Si(Condatos(Mavi.RM0188TipoMovimiento),<T> And V.MOV in(<T>+Mavi.RM0188TipoMovimiento+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CanalVtaCatxUEN),<T> And VCM.ID in(<T>+Mavi.RM0188CanalVtaCatxUEN+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CatCanal),<T> And VCM.Categoria =<T>+Comillas(Mavi.RM0188CatCanal),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtCatLigGrup),<T> AND A.Categoria=<T>+Comillas(Mavi.RM0188ArtCatLigGrup),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtGrupLigFam),<T> AND A.Grupo=<T>+Comillas(Mavi.RM0188ArtGrupLigFam),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtFamLigLin),<T> And A.Familia in(<T>+Mavi.RM0188ArtFamLigLin+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtLinLigLin),<T> And A.Linea in(<T>+Mavi.RM0188ArtLinLigLin+<T>)<T>,<T><T>)}<BR><BR><TAB>UNION ALL<BR><BR><TAB> Select V.UEN,V2.MOV,V2.MOVID,A.PROVEEDOR,P.NOMBRE,TIENDA=V.SucursalVenta,NOMSUCU=S.NOMBRE,V.CLIENTE,V.ENVIARA,FECHAEMISION=DATEADD(DD,0,DATEDIFF(DD,0,V2.FechaMavi)),<BR><TAB><TAB>VD.ARTICULO,CONCEPTO = V.CONDICION,V2.CANTIDAD,V.ESTATUS,A.DESCRIPCION1,PM.CostoFinal,Precio=VD.Precio/(1+(VD.Impuesto1/100)),VCM.Categoria,VCM.ID,<BR><TAB><TAB>Agente=V2.AgteVendedor,IVAACT=(SELECT DEFIMPUESTO FROM EmpresaGral WITH(NOLOCK) WHERE Empresa=<T>MAVI<T>) * .01,V2.Origen,V2.OrigenID,PArt.Costo,<BR><TAB><TAB>VD.RenglonTipo,ArtCategoria = A.Categoria,ArtGrupo = A.Grupo,ArtFamilia = A.Familia,ArtLinea = A.Linea,sl.SerieLote, Pdesc=0, Xfecha=CONVERT(datetime,CONVERT(varchar(10),GETDATE(),101),101),NombreCte=<T><T>,Autoriza=<T><T>,UsuarioDescuento=<T><T>,Normal=0, V.AgenteServicio,V.FormaCobro<BR><TAB> FROM(<BR><TAB><TAB><TAB>SELECT DISTINCT Cantidad,V.Mov,Cliente,EnviarA,Estatus,Uen,MovID,FechaMavi,Condicion,AgteVendedor,<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB>Origen,OrigenID,V.Articulo<BR><TAB><TAB><TAB>FROM V_MaviVentasNetas V WITH(NOLOCK)<BR><TAB><TAB><TAB>INNER JOIN MovTipo MT WITH(NOLOCK) ON MT.Mov=V.Mov AND MT.Modulo=<T>VTAS<T> AND MT.Clave=<T>VTAS.D<T><BR><TAB><TAB><TAB>WHERE 1=1 AND Origen like <T>Fac%<T><BR><TAB><TAB><TAB>{SI(Mavi.RM0188TipoReporte=<T>Operaciones Brutas<T>,<T> AND V.MOV=<T>+Comillas(<T>NINGUNA<T>),<T><T>)}<BR><TAB><TAB>) V2<BR><TAB><TAB>INNER JOIN Venta V WITH(NOLOCK) ON V.Mov=V2.Origen AND V.MovID=V2.OrigenID <BR><TAB><TAB>LEFT JOIN VENTAD VD WITH(NOLOCK) ON VD.ID = V.ID AND V2.Articulo=VD.Articulo <BR><TAB><TAB>LEFT JOIN PropreArt PArt ON Part.Art = VD.Articulo<BR><TAB><TAB>LEFT JOIN ART A WITH(NOLOCK) ON A.ARTICULO = VD.ARTICULO<BR><TAB><TAB>LEFT JOIN CTE C WITH(NOLOCK) ON C.CLIENTE = V.CLIENTE<BR><TAB><TAB>LEFT JOIN PROV P WITH(NOLOCK) ON P.PROVEEDOR = A.PROVEEDOR<BR><TAB><TAB>LEFT JOIN SUCURSAL S WITH(NOLOCK) ON S.SUCURSAL = V.SucursalVenta<BR><TAB><TAB>LEFT JOIN ventascanalmavi VCM WITH(NOLOCK) ON  VCM.ID = V.ENVIARA<BR><TAB><TAB>LEFT JOIN PlaneadorMacroHistMAVI PM WITH(NOLOCK) on VD.Articulo = PM.Codigo And PM.Proveedor = A.Proveedor<BR><TAB><TAB>LEFT JOIN SerieLoteMov SL WITH(NOLOCK) on sl.Id = vd.id and vd.renglonid = sl.renglonid AND SL.Modulo=<T>VTAS<T> AND Vd.Articulo=Sl.Articulo<BR><TAB>WHERE 1=1 <BR><TAB><TAB>{SI(ConDatos(Info.UEN),<T> AND V.UEN=<T>+Info.UEN,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.ProveedorD),<T> AND A.PROVEEDOR =<T>+Comillas(Info.ProveedorD),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188Condicion),<T> And V.CONDICION in(<T>+Mavi.RM0188Condicion+<T>)<T>,<T><T>)}<BR><TAB><TAB>AND<TAB>DATEADD(DD,0,DATEDIFF(DD,0,V2.FechaMavi)) Between <T>{FechaFormatoServidor(Info.FechaD)}<T> AND <T>{FechaFormatoServidor(Info.FechaA)}<T><BR><TAB><TAB>{Si(Condatos(Mavi.RM0188TipoMovimiento),<T> And V2.Origen in(<T>+Mavi.RM0188TipoMovimiento+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CanalVtaCatxUEN),<T> And VCM.ID in(<T>+Mavi.RM0188CanalVtaCatxUEN+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188CatCanal),<T> And VCM.Categoria =<T>+Comillas(Mavi.RM0188CatCanal),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtCatLigGrup),<T> AND A.Categoria=<T>+Comillas(Mavi.RM0188ArtCatLigGrup),<T><T>)}<BR><TAB><TAB>{SI(ConDatos(Mavi.RM0188ArtGrupLigFam),<T> AND A.Grupo=<T>+Comillas(Mavi.RM0188ArtGrupLigFam),<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtFamLigLin),<T> And A.Familia in(<T>+Mavi.RM0188ArtFamLigLin+<T>)<T>,<T><T>)}<BR><TAB><TAB>{Si(Condatos(Mavi.RM0188ArtLinLigLin),<T> And A.Linea in(<T>+Mavi.RM0188ArtLinLigLin+<T>)<T>,<T><T>)}<BR>)Tod<BR>GROUP BY UEN,MOV,MOVID,ORIGEN,ORIGENID,PROVEEDOR,NOMBRE,TIENDA,NOMSUCU,CLIENTE,ENVIARA,FECHAEMISION,ARTICULO,CONCEPTO,<BR>ESTATUS,DESCRIPCION1,CostoFinal,Costo,Precio,Categoria,ID,Agente,IVAACT,CAntidad,RenglonTipo,ArtCategoria,ArtGrupo,<BR>ArtFamilia,ArtLinea,SerieLote, PDesc, XFecha, NombreCte,autoriza,UsuarioDescuento,Normal, AgenteServicio,FormaCobro<BR>ORDER BY TIENDA, FECHAEMISION, CLIENTE, CONCEPTO
