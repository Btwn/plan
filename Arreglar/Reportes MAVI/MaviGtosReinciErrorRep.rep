SQL=/*DECLARE <BR><TAB>@FechaGD datetime,<BR><TAB>@FechaGA datetime,<BR><TAB>@FechaRD datetime,<BR><TAB>@FechaRA datetime<BR><TAB>SET @FechaGD = <T>{Info.FechaD}<T><BR><TAB>SET @FechaGA = <T>{Info.FechaA}<T><BR><TAB>SET @FechaRD = <T>{Mavi.FechaI}<T><BR><TAB>SET @FechaRA = <T>{Mavi.FechaF}<T><BR>*/<BR>SELECT<BR><TAB>G.UltimoCambio,<BR><TAB>G.FechaEmision,<BR>        NumSucursal=G.Sucursal,<BR><TAB>SituacionNota = LTrim(G.SituacionNota),<BR><TAB>G.SituacionNota,<BR><TAB>G.Importe,<BR><TAB>G.Retencion,<BR><TAB>G.Impuestos,<BR><TAB>G.Saldo,<BR><TAB>G.Estatus,<BR>        PNombre = LTrim(P.Nombre),<BR><TAB>G.Acreedor,<BR><TAB>G.ID,<BR><TAB>G.Empresa,<BR><TAB>G.Mov,<BR><TAB>G.MovID,<BR><TAB>G.UEN,<BR><TAB>G.Observaciones<BR>FROM<BR><TAB>Gasto G<BR><TAB>INNER JOIN Prov P ON P.Proveedor=G.Acreedor<BR><CONTINUA>
SQL002=<CONTINUA>WHERE<BR><TAB>G.Estatus=<T>PENDIENTE<T><BR><TAB>AND G.Saldo>0<BR>       {Si(ConDatos(Mavi.Acreedor),<T>AND G.Acreedor =<T>+comillas(Mavi.Acreedor),<T><T>)}<BR>/*<TAB>AND G.FechaEmision BETWEEN @FechaGD AND @FechaGA<BR><TAB>AND (G.SituacionFecha BETWEEN @FechaRD AND @FechaRA OR G.SituacionFecha is NULL)<BR>*/
