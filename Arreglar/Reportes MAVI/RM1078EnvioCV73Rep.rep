SQL=SELECT <BR><TAB>RFC,<BR><TAB>NOMBRE,<BR><TAB>IMPORTE,<BR><TAB>VIGENCIA = VIGENCIAAZO + QUINCENA,<BR><TAB>QUINCENAT = <T>   <T>+Ejercicio+<T>/<T>+Periodo,<BR><TAB>MOVIMIENTO<BR><TAB><BR>FROM (<BR><TAB>SELECT<BR><TAB><TAB>RFC = CteEA.RFCInstitucion,<BR><TAB><TAB>NOMBRE = Cte.Nombre,<BR><TAB><TAB>IMPORTE = MEDE.DescCte/2,<BR><TAB><TAB>VIGENCIA = MEDE.FechaEmision,<BR><TAB><TAB>QUINCENA = CONVERT(VARCHAR,dbo.Fn_MaviNumQuincena(DATEADD(mm,MEDE.NoDocs,MEDE.FechaEmision))),<BR><TAB><TAB>VIGENCIAAZO = CONVERT(VARCHAR,YEAR(DATEADD(mm,MEDE.NoDocs,MEDE.FechaEmision))),<BR><TAB><TAB>MOVIMIENTO = CASE WHEN CTEEA.Cargo IN (<T>federal<T>,<T>regularizado<T>) AND MEDE.TipoEnvio = <T>B<T> THEN <T>E<T> ELSE MEDE.TipoEnvio END,<BR><TAB><TAB>SALDO = SUM(MEDE.DescCte) OVER(PARTITION BY CteEA.RFCInstitucion),<BR<CONTINUA>
SQL002=<CONTINUA>><TAB><TAB>SALDOS = MEDE.DescCte,<BR><TAB><TAB>CONT = ROW_NUMBER() OVER (PARTITION BY MEDE.CLIENTE,MEDE.TipoEnvio  ORDER BY CteEA.RFCInstitucion DESC),<BR><TAB><TAB>CTEEA.Cargo,<BR><TAB><TAB>Ejercicio=CONVERT(VARCHAR(5),MEDE.Ejercicio),<BR><TAB><TAB>Periodo=CONVERT(VARCHAR(5),((MEDE.Periodo-1)*2)+MEDE.Quincena)<BR><TAB>FROM MaviEnvDispElectHist MEDE<BR><TAB><TAB>INNER JOIN CteEnviarA CteEA ON CteEA.Id = MEDE.IdCanalVenta AND CteEA.Cliente = MEDE.Cliente<BR><TAB><TAB>INNER JOIN Cte Cte ON Cte.Cliente = MEDE.Cliente<BR><TAB>WHERE MEDE.IDCanalVenta=73 <BR><TAB><TAB>{Si(ConDatos(Info.Ejercicio),<T> And MEDE.Ejercicio=<T>+Info.Ejercicio,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.Periodo),<T> And MEDE.Periodo=<T>+Info.Periodo,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.Quincena),<T> And MEDE.Quincena=<<CONTINUA>
SQL003=<CONTINUA>T>+Info.Quincena,<T><T>)}<BR><TAB><TAB>{si(condatos(Mavi.RM1078Subdependencias),(CASO Mavi.RM1078Subdependencias ES <T>Otros...<T> ENTONCES <T> and CteEA.CARGO NOT IN (<T>+COMILLAS(<T>Federal<T>)+<T>,<T>+COMILLAS(<T>Estatal<T>)+<T>,<T>+COMILLAS(<T>Regularizado<T>)+<T>)<T> <BR><TAB><TAB> SINO <T> AND CteEA.CARGO=<T>+comillas(Mavi.RM1078Subdependencias)),<T><T>)}<BR>)T<BR>WHERE CONT = 1<BR>group by RFC,<BR><TAB>NOMBRE,<BR><TAB>IMPORTE,<BR><TAB>VIGENCIAAZO ,<BR><TAB>QUINCENA,<BR><TAB>Ejercicio,Periodo,<BR><TAB>MOVIMIENTO,<BR><TAB>CARGO<BR>ORDER BY RFC
