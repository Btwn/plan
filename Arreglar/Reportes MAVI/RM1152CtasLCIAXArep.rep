SQL=declare @fechad datetime, <BR>@fechaa datetime<BR>SET @fechaD = <T>{Fechaformatoservidor(Info.FechaD)}<T><BR>SET @fechaA = <T>{Fechaformatoservidor(Info.FechaA)}<T><BR>SELECT <BR>SUC=CONVERT(varchar,v.origensucursal),<BR>CANALVENTA=CONVERT(varchar,v.enviara),<BR>FECHA1ERFACT=CONVERT(varchar,v.fechaemision,111),<BR>FECHAULTFACT=CONVERT(varchar,vu.fechaemision,111),v.CLIENTE,c.NOMBRE,<BR>EDAD= convert(varchar,DATEDIFF(YEAR,c.fechanacimiento,GETDATE()) - (CASE WHEN DATEADD(YY,DATEDIFF(YEAR,c.fechanacimiento,GETDATE()),c.fechanacimiento)>GETDATE() THEN 1 ELSE 0 END)), <BR>s.ESTADO,V.CONDICION,<BR>IMPORTE=<T>$<T>+CAST(CONVERT(varchar, CAST(V.importe+V.impuestos AS money), 1) AS varchar),<BR>LCIAXA=<T>$<T>+CAST(CONVERT(varchar, CAST(c.crmimporte AS money), 1) AS varchar),<BR>SALDO=<T>$<T>+CAST(C<CONTINUA>
SQL002=<CONTINUA>ONVERT(varchar, CAST(ISNULL(sdo.saldo,0) AS money), 1) AS varchar),<BR>FECHA1ERABONO=convert(varchar,co.fechapago1er,111), <BR>FECHAULTABONO=convert(varchar,co.fechapagoult,111),<BR>POLITICACREDITO=ISNULL(c.credito,<T><T>),<BR>TOTMOVTOS=CONVERT(varchar,ISNULL(sdo.movs,0)),<BR>MOVTOSDO=CONVERT(varchar,ISNULL(sdo.movsdo,0)),<BR>TIPOCREDITO=UPPER(isnull(tc.textoamostrar,<T><T>)),<BR>MHDV=CONVERT(varchar,ISNULL(dbo.fnMaviMaxMaxDiasVencidosCte(v.Cliente,<T>CREDITO MENUDEO<T>),0)),<BR>AUTCREDESPECIAL=CASE WHEN c.esAutCredEspecial= 1 THEN <T>SI<T> ELSE <T>NO<T> END<BR>FROM VENTA v with (nolock) <BR>JOIN movtipo mt with (nolock) on v.mov = mt.mov and mt.modulo = <T>vtas<T> and mt.clave in(<T>vtas.f<T>)<BR>JOIN CTE C with (nolock) on c.cliente = v.cliente<BR>JOIN SUCURSAL S with (nolock) on v.orige<CONTINUA>
SQL003=<CONTINUA>nsucursal = s.sucursal<BR>JOIN VENTASCANALMAVI VC WITH (NOLOCK) ON V.ENVIARA = VC.ID  <BR>LEFT JOIN DM0299TIPODEcLIENTE TC with (nolock) on c.tipocredito = tc.id<BR>LEFT JOIN CXC cc with (nolock) on cc.mov = v.mov and cc.movid = v.movid<BR>LEFT JOIN (<BR><TAB>SELECT CLIENTE,SALDO=SUM(SALDO),MOVS=COUNT(*),MOVSDO=sum(CASE WHEN saldo > 0 then 1 else 0 end)<BR><TAB>FROM (<TAB><BR><TAB>    SELECT cliente,cc.origen,cc.origenid,Saldo=ROUND(SUM(isnull(CASE WHEN ORIGEN = <T>Seguro Vida<T> THEN Saldo / 10 else Saldo end,0)),2) <BR><TAB>    FROM Cxc cc With(NoLock)<BR><TAB>    JOIN Movtipo mt With(NoLock) ON Cc.mov = Mt.mov AND Mt.Modulo = <T>Cxc<T>  and mt.clave = (<T>cxc.d<T>)<BR><TAB>    JOIN VENTASCANALMAVI VC WITH (NOLOCK) ON cc.ClienteEnviarA = VC.ID  <BR><TAB>    WHERE VC.CATEGORIA = <T>CREDIT<CONTINUA>
SQL004=<CONTINUA>O MENUDEO<T><BR>              AND CC.ESTATUS IN (<T>PENDIENTE<T>,<T>CONCLUIDO<T>)<BR><TAB>    GROUP BY cc.cliente,cc.origen,Cc.OrigenID<BR><TAB>    ) T <BR>        GROUP BY T.cliente<BR><TAB> ) sdo ON Sdo.Cliente = v.cliente <BR>LEFT JOIN (<BR><TAB>SELECT * FROM (<BR><TAB><TAB>SELECT ROW_NUMBER() OVER(PARTITION BY v.cliente ORDER BY v.cliente,v.id DESC) AS venta,cLIENTE,FECHAEMISION <BR><TAB><TAB>FROM VENTA v with (nolock) <BR><TAB><TAB>JOIN movtipo mt with (nolock) on v.mov = mt.mov and mt.modulo = <T>vtas<T> and mt.clave in(<T>vtas.f<T>)<BR><TAB><TAB>JOIN VENTASCANALMAVI VC WITH (NOLOCK) ON V.ENVIARA = VC.ID  <BR><TAB><TAB>WHERE V.ESTATUS = <T>CONCLUIDO<T> <BR><TAB><TAB><TAB>AND VC.CATEGORIA = <T>CREDITO MENUDEO<T><BR><TAB><TAB>) X WHERE venta = 1<BR><TAB>) VU ON VU.CLIENTE = V.CLIENTE<B<CONTINUA>
SQL005=<CONTINUA>R>LEFT JOIN (<BR><TAB>SELECT CLIENTE,FechaPago1er,FechaPagoUlt FROM DBO.FnRM1152PRIUltPagoXCliente(@fechaD,@fechaA)<BR><TAB>   ) co ON co.CLIENTE = V.CLIENTE   <BR><BR>LEFT JOIN  (<BR><TAB>SELECT v.mov,v.movid,v.referencia<BR><TAB>FROM VENTA v WITH (NOLOCK)<BR><TAB>WHERE v.mov like  <T>Devolucion V%<T> and v.origentipo = <T>vtas<T> <BR><TAB><TAB>and v.estatus = <T>concluido<T> and v.referencia is not null<TAB><BR>     <TAB>) DEV ON dev.referencia = v.mov+<T> <T>+v.movid <BR> WHERE V.ESTATUS = <T>CONCLUIDO<T> <BR>  AND mavitipoventa =<T>Nuevo<T> <BR>  AND V.FECHAEMISION BETWEEN @fechaD and @fechaA<BR>  AND dev.referencia is null<BR>  AND VC.CATEGORIA = <T>CREDITO MENUDEO<T><BR>  {Si(ConDatos(Mavi.Sucursal),<T> AND v.OrigenSucursal=<T>+Mavi.Sucursal,<T><T>)}<BR>  {Si(ConDatos(Mavi.RM1152Esta<CONTINUA>
SQL006=<CONTINUA>do),<T> AND s.Estado=<T>+Comillas(Mavi.RM1152Estado),<T><T>)}<BR>  {Si(ConDatos(Mavi.RM1152PoliticaCredito),<T> AND c.Credito=<T>+Comillas(Mavi.RM1152PoliticaCredito),<T><T>)}<BR>  {Si(ConDatos(Mavi.RM1152TipoCredito),<T> AND c.TipoCredito=<T>+Mavi.RM1152TipoCredito,<T><T>)}<BR>  Order by v.fechaemision,v.OrigenSucursal
