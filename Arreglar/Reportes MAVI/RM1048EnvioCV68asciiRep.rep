SQL002=<CONTINUA> OVER(PARTITION BY CteEA.RFCInstitucion),<BR><TAB><TAB>SALDOS = MEDE.DescCte,<BR><TAB><TAB>CONT = ROW_NUMBER() OVER (PARTITION BY MEDE.CLIENTE,MEDE.TipoEnvio ORDER BY CteEA.RFCInstitucion DESC),<BR><TAB><TAB>CteEA.CARGO<BR><TAB>FROM MaviEnvDispElectHist MEDE<BR><TAB><TAB>INNER JOIN CteEnviarA CteEA ON CteEA.Id=MEDE.IdCanalVenta AND CteEA.Cliente = MEDE.Cliente<BR><TAB><TAB>INNER JOIN Cte Cte ON Cte.Cliente = MEDE.Cliente<BR><TAB>WHERE 1=1 AND MEDE.IDCanalVenta=68 <BR><TAB><TAB>{Si(ConDatos(Info.Ejercicio),<T> And MEDE.Ejercicio=<T>+Info.Ejercicio,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.Periodo),<T> And MEDE.Periodo=<T>+Info.Periodo,<T><T>)}<BR><TAB><TAB>{Si(ConDatos(Info.Quincena),<T> And MEDE.Quincena=<T>+Info.Quincena,<T><T>)}<BR><TAB><TAB>{si(condatos(Mavi.RM1048Subdependencias),(CASO M<CONTINUA>
