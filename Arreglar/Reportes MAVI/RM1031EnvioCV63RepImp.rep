SQL=SELECT<BR><TAB>T.RFC,<BR><TAB>T.FIJO,<BR><TAB>DescQna = RIGHT(REPLICATE(<T>0<T>,8)+CONVERT(VARCHAR(8),T.DescQna),8),<BR><TAB>HASTA = CASE WHEN Mov = <T>B<T> THEN (SUBSTRING(CONVERT(VARCHAR,DATEADD(MM,1,GETDATE()),102),1,4)+ DBO.Fn_MaviNumQuincena(DATEADD(MM,1,GETDATE()))) ELSE MAX(F.FINAL) END,<BR><TAB>T.FIJO2,<BR><TAB>T.Mov,<BR><TAB>T.ClaveInst<BR>FROM (<BR>SELECT <BR><TAB>RFC     = CONVERT(VARCHAR(13),ISNULL(C.RFCInstitucion,<T><T>)+ <T>------------<T>),<BR><TAB>FIJO    = <T> TR1 = <T>+CHAR(123)+<T>246<T> ,<BR><TAB>DescQna = CONVERT(DECIMAL(8,2),(H.DescCte/2)),<BR><TAB>QnaFin  = (SUBSTRING(CONVERT(VARCHAR,DATEADD(MM,H.NoDocs,H.FechaEmision),102),1,4)+ DBO.Fn_MaviNumQuincena(DATEADD(MM,H.NoDocs,H.FechaEmision))),<BR><TAB>FIJO2   = <T>MA<T>+CHAR(125),<BR><TAB>Mov<TAB>= CASE H.TipoEnvio WHE<CONTINUA>
SQL002=<CONTINUA>N <T>M<T> THEN <T>C<T> ELSE H.TipoEnvio END,<BR><TAB>c.ClaveInst,<BR><TAB>CTE.Nombre<BR>FROM MaviEnvDispElectHist H<BR>INNER JOIN dbo.CteEnviarA C ON H.IDCanalVenta = C.ID AND H.Cliente = C.Cliente<BR>INNER JOIN CTE ON CTE.Cliente = H.Cliente<BR>WHERE H.IDCanalVenta = 63 AND c.ClaveInst LIKE <T>416%<T><BR><BR>{Si(ConDatos(Info.Ejercicio),<T> And H.Ejercicio=<T>+Info.Ejercicio,<T><T>)}<BR>{Si(ConDatos(Info.Periodo),<T> And H.Periodo=<T>+Info.Periodo,<T><T>)}<BR>{Si(ConDatos(Info.Quincena),<T> And H.Quincena=<T>+Info.Quincena,<T><T>)}<BR><BR>)T<BR><BR>LEFT JOIN (<BR><TAB><TAB><TAB>SELECT <BR><TAB><TAB><TAB><TAB>NOMBRE,<BR><TAB><TAB><TAB><TAB>TipoEnvio,<BR><TAB><TAB><TAB><TAB>FINAL= SUBSTRING( CONVERT(VARCHAR,(DATEADD(dd,15,MAX(FIN))),102), 1,4) + DBO.Fn_MaviNumQuincena(DATEADD(dd,15,MAX(FIN)<CONTINUA>
SQL003=<CONTINUA>))<BR><BR><TAB><TAB><TAB>FROM(<BR><TAB><TAB><TAB><TAB>SELECT <BR><TAB><TAB><TAB><TAB><TAB>cte.Nombre,<BR><TAB><TAB><TAB><TAB><TAB>FIN = MAX(DATEADD(MM,H.NoDocs,H.FechaEmision)),<BR><TAB><TAB><TAB><TAB><TAB>H.TipoEnvio<BR><TAB><TAB><TAB><TAB>FROM MaviEnvDispElectHist H WITH(NOLOCK)<BR><TAB><TAB><TAB><TAB>INNER JOIN CTE WITH(NOLOCK)ON CTE.Cliente = H.Cliente<BR><TAB><TAB><TAB><TAB>GROUP BY CTE.Nombre,H.FechaEmision,H.TipoEnvio<BR><TAB><TAB><TAB><TAB>)KK<BR><TAB><TAB><TAB>GROUP BY KK.Nombre,KK.TipoEnvio,kk.FIN<BR><TAB><TAB>  )F ON F.NOMBRE = T.Nombre<BR><BR>GROUP BY T.RFC,T.FIJO,T.DescQna,T.FIJO2,T.MOV,t.claveinst<BR>Order By Mov,RFC
