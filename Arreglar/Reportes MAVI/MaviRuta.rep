SQL=SELECT <BR> MRS.ID,<BR> MRS.Empresa,<BR> MRS.Mov, <BR> MRS.MovID, <BR> MRS.FechaEmision, <BR> MRS.UEN, MRS.Agente, <BR> MRS.Ruta, <BR> MRS.Usuario, <BR> MRS.Referencia,<BR> MRS.Observaciones, <BR> MRS.Estatus, <BR> MRS.Sucursal, <BR> MRS.DID, <BR> MRS.SUPERID,<BR> MRS.Estado, <BR> MRS.MRDOBSERVACIONES,<BR> MRS.MRDSUPERMOV,<BR> MRS.MSObs,<BR> MRS.ESTATUSDET,<BR> MRS.DETALLERUTA,<BR> MRS.MRDCLIENTE, <BR> MRS.MRDNOMBRECLIENTE, <BR> MRS.Tipo, <BR> MRS.CteCto,<BR> MRS.CtoNombre,<BR> MRS.FechaSupervision,<BR> NOMBREAGT=AGENTE.NOMBRE <BR>FROM (SELECT <BR> MR.ID,<BR> MR.Empresa,<BR> MR.Mov, <BR> MR.MovID, <BR> MR.FechaEmision, <BR> MR.UEN, MR.Agente, <BR> MR.Ruta, <BR> MR.Usuario, <BR> MR.Referencia,<BR> MR.Observaciones, <BR> MR.Estatus, <BR> MR.Sucursal, <BR> MRD.DID, <BR> SUPERID=MRD.MOVID,<BR><CONTINUA>
SQL002=<CONTINUA> MRD.Estado, <BR> MRDOBSERVACIONES=MRD.Observaciones,<BR> MRDSUPERMOV=MRD.Mov,<BR> MRD.MSObs,<BR> ESTATUSDET=MRD.Estatus,<BR> DETALLERUTA=MRD.RutaID,<BR> MRDCLIENTE=MRD.Cliente, <BR> MRDNOMBRECLIENTE=MRD.Nombre, <BR> MRD.Tipo, <BR> MRD.CteCto,<BR> MRD.CtoNombre,<BR> MRD.FechaSupervision<BR>FROM <BR> MaviRuta MR <BR>INNER JOIN(SELECT<BR>   DID=MaviRutaD.ID,<BR>   MaviRutaD.SupervisionID,<BR>   MaviRutaD.Estado, <BR>   MaviRutaD.Observaciones,<BR>   MaviSupervision.ID, <BR>   MaviSupervision.Mov,<BR>   MaviSupervision.MovID,<BR>   MaviSupervision.FechaEmision,<BR>   MaviSupervision.FechaRegistro, <BR>   MaviSupervision.FechaConclusion,<BR>   MaviSupervision.FechaCancelacion,<BR>   MaviSupervision.Usuario, <BR>   MSObs=MaviSupervision.Observaciones,<BR>   MaviSupervision.Estatus,<BR>   MaviSu<CONTINUA>
SQL003=<CONTINUA>pervision.RutaID,<BR>   MaviSupervision.Sucursal, <BR>   MaviSupervision.Cliente, <BR>   MaviSupervision.Nombre, <BR>   MaviSupervision.NombreConyuge,<BR>   MaviSupervision.Tipo,<BR>   MaviSupervision.CteCto, <BR>   MaviSupervision.CtoNombre, <BR>   MaviSupervision.Agente,<BR>   MaviSupervision.FechaSupervision, <BR>   MaviSupervision.Comentarios<BR>  FROM<BR>   MaviRutaD <BR>  LEFT OUTER JOIN<BR>   MaviSupervision ON MaviRutaD.SupervisionID = MaviSupervision.ID) AS MRD ON MRD.DID = MR.ID)MRS<BR>INNER JOIN Agente ON MRS.Agente = Agente.Agente
