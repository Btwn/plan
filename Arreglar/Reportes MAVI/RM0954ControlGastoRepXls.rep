SQL=SELECT<BR>X.Proveedor<TAB><BR>,X.PERSONAL<BR>,X.NOMBRE<BR>,X.APELLIDOPATERNO<BR>,X.APELLIDOMATERNO<BR>,X.PROVCUENTA<BR>,X.IMPORTE<BR>,X.ORIGENID MOVID<BR>,X.MOV<BR>,comprobanteInst<BR>,SolicitudCheque<BR>,X.SEMANA<BR>,X.ANO<BR><BR>FROM(<BR><TAB>SELECT <BR><TAB>P.Proveedor<TAB><BR><TAB>,PER.PERSONAL<BR><TAB>,PER.NOMBRE<BR><TAB>,PER.APELLIDOPATERNO<BR><TAB>,PER.APELLIDOMATERNO<BR><TAB>,P.PROVCUENTA<BR><TAB>,IMPORTE=(G.IMPORTE+G.IMPUESTOS) - (Isnull(G.Retencion,0))<BR><TAB>,OrigenID=ISNULL(G.ORIGENID,G.MovAplicaID)<BR><TAB>,G.MOV<BR><TAB>,comprobanteInst=G.MovID<BR><TAB>,SolicitudCheque=Case When C.Mov IS Not null Then D.MovID + <T> ,<T>+C.MovID Else D.MovID End<BR><TAB>,dbo.Fn_MaviRM0952ObtenSemana(G.FECHAEMISION) SEMANA<BR><TAB>,convert(int,DATEPART(YEAR,G.FECHAEMISION)) ANO<BR><BR><BR><TAB<CONTINUA>
SQL002=<CONTINUA>>FROM GASTO G<BR><BR><TAB>INNER JOIN PROV P With(NoLock) ON P.PROVEEDOR=G.ACREEDOR<BR><TAB>LEFT JOIN V_MAVIPERSONAL PER With(NoLock) ON PER.PERSONAL=P.AGENTE<BR><TAB>LEFT JOIN Dinero D With(NoLock) ON Isnull(G.Origen,G.MovAplica) =D.Origen and ISNULL(G.OrigenID,G.MovAplicaID) =D.OrigenID and D.Mov=<T>Solicitud Cheque<T> and D.OrigenTipo=<T>GAS<T><BR><TAB>LEFT JOIN Dinero C With(NoLock) ON G.Mov=C.Origen and G.MovID=C.OrigenID and C.Mov=<T>Solicitud Cheque<T> and C.OrigenTipo=<T>GAS<T> <BR><TAB>WHERE<TAB>/*AG.CATEGORIA=<T>VENTAS A INSTITUCIONES<T> <BR><TAB><TAB><TAB>AND P.CATEGORIA=<T>INSTITUCIONES<T><TAB><TAB><BR><TAB><TAB><TAB>AND*/ G.MOV IN (<T>Comprobante Inst<T>)<BR><TAB><TAB><TAB>AND G.ESTATUS=<T>CONCLUIDO<T><BR><BR><BR>)X<BR>WHERE ANO={INFO.ANO} AND SEMANA={MAVI.RM0954SEMANA}<TAB><TA<CONTINUA>
