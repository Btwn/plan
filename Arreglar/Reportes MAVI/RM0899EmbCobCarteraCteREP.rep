SQL=WITH Todo AS (<BR>Select * from<BR>(<BR>SELECT <BR><TAB>Embarque.Ruta, <BR><TAB>Embarque.FechaEmision, <BR><TAB>DelegacionRutaOrden=R.Destino,<BR><TAB>DelegacionRuta=R.Delegacion,<BR><TAB>DelegacionCliente=Cte.Delegacion,<BR><TAB>EmbarqueMov.Cliente,  <BR><TAB>NombredelCliente=Cte.Nombre,<BR><TAB>EmbarqueMov.NombreEnvio,<BR><TAB>Embarque.ID,<BR><TAB>Embarque.Empresa,<BR><TAB>EmbarqueMov = Embarque.Mov,<BR><TAB>EmbarqueMovID = Embarque.MovID,<BR><TAB>EmbarqueMoneda = Embarque.Moneda,<BR><TAB>Embarque.Estatus,<BR><TAB>Embarque.Vehiculo,<BR><TAB>Embarque.Agente,<BR><TAB>Embarque.OrigenTipo,<BR><TAB>Embarque.Origen,<BR><TAB>Embarque.OrigenID,<BR><TAB>Embarque.Ejercicio,<BR><TAB>Embarque.Periodo,<BR><TAB>Embarque.FechaRegistro,<BR><TAB>Embarque.FechaSalida,<BR><TAB>Embarque.Importe,<BR><TAB>Emb<CONTINUA>
SQL002=<CONTINUA>arque.Sucursal,<BR><TAB>EmbarqueD.Orden,<BR>        EmbarqueD.Estado,<BR><TAB>EmbarqueD.FechaHora,<BR><TAB>EmbarqueDReferencia = EmbarqueD.Referencia,<BR><TAB>EmbarqueMov.AsignadoID,<BR><TAB>EmbarqueMov.ModuloID,<BR><TAB>EmbarqueMovMov = EmbarqueMov.Mov,<BR><TAB>EmbarqueMovMovID = EmbarqueMov.MovID,<BR>        EmbarqueMov.MovReferencia,<BR>        EmbarqueMov.MovEstatus,<BR><TAB>EmbarqueMov.ClienteEnviarA,<BR><TAB>EmbarqueMovImporte = (EmbarqueMov.Importe *1.16),<BR><TAB>EmbarqueMovImpuestos = EmbarqueMov.Impuestos,<BR><TAB>EmbarqueMov.Vencimiento,<BR><TAB>Nombre=EmbarqueMov.Nombre,<BR><TAB>Vehiculo.Descripcion,<BR><TAB>AgenteNombre = Agente.Nombre,<BR><TAB>TipoFactura=CXC.Origen,<BR><TAB>FacturaNo=CXC.OrigenID,<BR><TAB>HrEntregado=EmbarqueD.FechaHora,<BR><TAB>RutaEmbarque=EmbarqueMov.Ruta<CONTINUA>
SQL003=<CONTINUA>,<BR>        BuscaPar = CHARINDEX(<T>(<T>,MovReferencia),<BR>        BuscarDia =CHARINDEX(<T>/<T>,MovReferencia),<BR>        buscarPAr2 = CHARINDEX(<T>)<T>,MovReferencia),<BR>        num = SUBSTRING(MovReferencia,CHARINDEX(<T>(<T>,MovReferencia)+1,CHARINDEX(<T>/<T>,MovReferencia)-CHARINDEX(<T>(<T>,MovReferencia)-1),<BR>       num1 = SUBSTRING(MovReferencia,CHARINDEX(<T>/<T>,MovReferencia)+1,CHARINDEX(<T>)<T>,MovReferencia)-CHARINDEX(<T>/<T>,MovReferencia)-1)<BR>,Abono = (CXC.IMPORTE + CXC.IMPUESTOS)<BR>FROM Embarque<BR>  JOIN EmbarqueD ON Embarque.ID = EmbarqueD.ID<BR>  JOIN EmbarqueMov ON EmbarqueD.EmbarqueMov = EmbarqueMov.ID<BR>  JOIN Cte ON EmbarqueMov.Cliente = Cte.Cliente<BR>  LEFT JOIN Vehiculo ON Embarque.Vehiculo = Vehiculo.Vehiculo <BR>  LEFT JOIN Agente ON Embarque.Agente = Agen<CONTINUA>
SQL004=<CONTINUA>te.Agente AND Agente.Estatus = <T>ALTA<T><BR>  LEFT JOIN Prov ON Embarque.Proveedor = Prov.Proveedor AND Prov.Estatus = <T>ALTA<T><BR>  JOIN MovTipo ON Embarque.Mov = Movtipo.Mov<BR>  LEFT JOIN  CXC ON EmbarqueMov.MOV = CXC.MOV AND EmbarqueMov.MOVID = CXC.MOVID<BR>left join <BR>(<BR>select Cliente, ID, Ruta, Delegacion from cteenviara where ID IN (<T>11<T>) )Cte2 on Cte2.Cliente=Cte.Cliente<BR>left join <BR>  (Select R.*, SUBSTRING(R.Destino,4, 1000) As <T>Delegacion<T><BR>From RutaDestinosMAVI R <BR><BR><BR>) R on R.Ruta=Cte2.Ruta and R.Delegacion=Cte2.Delegacion<BR><BR>WHERE (Embarque.Mov = <T>Orden Cobro Mayoreo<T> AND Embarque.Estatus = <T>Pendiente<T> and  EmbarqueMov.ClienteEnviarA in (<T>10<T>,<T>11<T>))<BR><BR>) Todo<BR>where 1=1<BR>{Si(ConDatos(Mavi.RM0899RutasEmbarques),<BR>    <<CONTINUA>
SQL005=<CONTINUA>T> And Todo.Ruta In(<T>+Mavi.RM0899RutasEmbarques+<T>)<T>,<BR>   <T><T>)}<BR>{Si((ConDatos(Info.FechaD)) y (ConDatos(Info.FechaA)),<T>And (Cast(FLOOR(CAST(Todo.FechaEmision AS real))as DateTime)) BETWEEN <T>+Comillas(FechaFormatoServidor(Info.FechaD))+<T> And <T>+Comillas(FechaFormatoServidor(Info.FechaA)),<T><T>)}<BR><BR>)<BR><BR>select distinct <BR>todo.*,Saldo.Saldo from Todo left join (select  mov,movid,padremavi,padreidmavi,cliente,  /*sum (saldo) as */ Saldo from cxcpendiente <BR><BR>where mov in (/*<T>nota cargo<T>, <T>nota de cargo mayoreo<T>, */<T>documento<T>) <BR><TAB> <BR>group by mov,movid,padremavi,padreidmavi,cliente, Saldo )saldo <BR>on Todo.embarquemovmov = saldo.mov and Todo.Cliente = saldo.Cliente and saldo.movid = todo.embarquemovmovid<BR><BR>Order By Ruta,DelegacionRut<CONTINUA>
