[SerieLote.vis/Vista]
SQL=SELECT<BR>  SerieLote.Sucursal,<BR>  SerieLote.Empresa,<BR>  SerieLote.Articulo,<BR>  SerieLote.SubCuenta,<BR>  SerieLote.SerieLote,<BR>  SerieLote.Almacen,<BR>  SerieLote.Tarima,<BR>  SerieLote.Propiedades,<BR>  SerieLote.Existencia,<BR>  SerieLote.ExistenciaAlterna,<BR>  SerieLote.ExistenciaActivoFijo,<BR>  SerieLote.UltimaEntrada,<BR>  SerieLote.UltimaSalida,<BR>  SerieLote.Cliente,<BR>  SerieLote.Localizacion,<BR>  SerieLote.CostoPromedio,<BR>  Art.Articulo,<BR>  Art.Descripcion1,<BR>  Art.Tipo,<BR>  SerieLoteProp.Propiedades,<BR>  SerieLoteProp.Extra1,<BR>  SerieLoteProp.Extra2,<BR>  SerieLoteProp.Extra3,<BR>  SerieLoteProp.Fecha1,<BR>  SerieLoteProp.Fecha2,<BR>  SerieLoteProp.Fecha3,<BR>  SerieLoteProp.Factor1,<BR>  SerieLoteProp.Factor2,<BR>  SerieLoteProp.Factor3,<BR>  SerieLoteProp.PedimentoClave,<BR>  SerieLoteProp.PedimentoRegimen,<BR>  SerieLoteProp.ValorDolares,<BR>  SerieLoteProp.ValorAduana,<BR>  SerieLoteProp.ValorComercial,<BR>  SerieLoteProp.AgenteAduanal,<BR>  SerieLoteProp.Aduana,<BR>  SerieLoteProp.DTA,<BR>  SerieLoteProp.IVA,<BR>  SerieLoteProp.IGI,<BR>  Art.Linea<BR><BR>FROM<BR>  SerieLote<BR>  JOIN Art ON SerieLote.Articulo=Art.Articulo<BR>  LEFT OUTER JOIN SerieLoteProp ON SerieLote.Propiedades=SerieLoteProp.Propiedades


[SerieLote.vis/Vista.ListaCampos]
SerieLoteProp.IGI=Art.Linea
Art.Linea=(Fin)

[SerieLote.vis/Vista.ListaCalculados]
CostoPromedioTotal=Modelo2
Modelo2=(Fin)

[SerieLote.vis/Modelo2]
Clave=Modelo2
Tamano=10
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Modelo
Formula=Si(Art.Linea='MOTOCICLETAS',SerieLoteProp.Extra3,'N/A')  

[SerieLote.vis/Art.Linea]
Clave=Art.Linea
LigaTabla=Art
LigaCampo=Linea
Ligado=S
TipoCampo=Tabla
TipoDatos=Texto
Tamano=50
ClaveBDE=Linea