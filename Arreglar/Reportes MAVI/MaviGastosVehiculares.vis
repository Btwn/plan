SQL003=<CONTINUA>tiembre, <T>$ <T> + ISNULL(CONVERT(Varchar, <BR>                                              ROUND(Octubre, 2)), <T>0.00<T>) AS Octubre, <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Noviembre, 2)), <T>0.00<T>) AS Noviembre, <BR>                                              <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Diciembre, 2)), <T>0.00<T>) AS Diciembre, <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Total, 2)), <T>0.00<T>) <BR>                                              AS Total, ISNULL(Proveedor, <T>N / A<T>) AS Nomina, ISNULL(Nombre, <T>No Asignado<T>) AS Nom_Chofer, FechaEmision AS Fecha, <BR>                                              Mov AS Documento, MovID AS Num_Documento, ISNULL(Acreedor, <T>Mavi<T>) AS Num_Proveedor, ISNULL(Nombre_1, <T>Mavi<T>) <BR>                                   <CONTINUA>
SQL004=<CONTINUA>           AS Nom_Proveedor, Referencia, Concepto, DescripcionExtra AS Observaciones, Clase AS Clasificacion, Cantidad, LecturaAnterior, Lectura, <BR>                                              KilometrosRecorridos, ROUND(Rendimiento, 2) AS Rendimiento, <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Importe, 2)), <T>0.00<T>) AS Importe, <BR>                                              <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Precio, 2)), <T>0.00<T>) AS Precio, <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Impuestos, 2)), <T>0.00<T>) <BR>                                              AS Impuestos, ISNULL(Nota, <T>Sin Nota<T>) AS Nota, <T>$ <T> + ISNULL(CONVERT(Varchar, ROUND(Impuestos + Importe, 2)), <T>0.00<T>) AS GranTotal, <BR>                                              CentroCostos, Serie<BR> <CONTINUA>
