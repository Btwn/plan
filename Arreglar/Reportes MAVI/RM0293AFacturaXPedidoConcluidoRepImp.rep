SQL=<BR>WITH X AS <BR><TAB><TAB>(<BR>         SELECT V.ORIGEN<BR>                ,ORIGENID<BR>                ,TIENDA = V.SUCURSALVENTA<BR>                ,NOMBRESUC = S.NOMBRE<BR>                ,PEDIDOMOV = V.MOV<BR>                ,NUMEROPED = V.MOVID<BR>                ,CA.CAUSA<BR>                ,FECHAPED = V.FECHAEMISION<BR>                ,V.FECHAREQUERIDA<BR>                ,NUMEROCTE = C.CLIENTE<BR>                ,NOMBRECTE = C.NOMBRE<BR>                ,AGENTE = V.AGENTE<BR>                ,ALM = MAX(V.ALMACEN)<BR>                ,CONDICION = Max(V.Condicion)<BR>                ,FormaPago = Max(V.FormaPagoTipo)<BR>                ,TELEFONO = Max(C.Telefonos)<BR>                ,IMPTPEDIDO = V.PrecioTotal<BR>                ,VD.ARTICULO<BR>                ,A.DESCRIPCION1<BR>        <CONTINUA>
SQL002=<CONTINUA>        ,VD.CANTIDAD<BR>                ,VD.PRECIO<BR>                ,MOVID<BR><BR><BR>                FROM VENTA V LEFT JOIN CTE C ON C.CLIENTE = V.CLIENTE LEFT JOIN EQUIPOAGENTE E on V.AGENTE = E.AGENTE <BR>                                LEFT JOIN SUCURSAL S on S.SUCURSAL=V.SUCURSALVENTA LEFT JOIN VENTAD VD ON V.ID=VD.ID<BR>                                   LEFT JOIN ART A ON VD.ARTICULO=A.ARTICULO LEFT JOIN CAUSA CA ON CA.Causa = V.Causa <BR>           <BR>                            Where V.MOV LIKE <T>FACTURA%<T> AND V.ESTATUS in (<T>CONCLUIDO<T>) <BR>                                       AND V.MOVID IS NOT NULL<BR>                                           <BR>                                      <BR><TAB><TAB><TAB><TAB><TAB>               <BR>                              GROUP<CONTINUA>
SQL003=<CONTINUA> BY V.SUCURSALVENTA,C.CLIENTE,S.NOMBRE,V.MOV,V.MOVID,V.FECHAEMISION,C.NOMBRE,V.AGENTE,<BR>                                           V.PrecioTotal,VD.ARTICULO,A.DESCRIPCION1,VD.CANTIDAD,VD.PRECIO,CA.CAUSA,V.FECHAREQUERIDA,<BR>                                               V.ORIGEN,ORIGENID<BR>       <BR>         )<BR><BR>           SELECT PEDIDOMOV = X.ORIGEN<BR>                  ,NUMEROPED = X.ORIGENID<BR>                  ,FECHAPED = V.FECHAEMISION<BR>                  ,TIENDA = V.SUCURSALVENTA<BR>                  ,NOMBRESUC = S.NOMBRE<BR>                  ,FACTURAMOV = X.PEDIDOMOV<BR>                  ,NUMEROFACT = X.NUMEROPED<BR>                  ,CA.CAUSA<BR>                  ,FECHAFACT = X.FECHAPED<BR>                  ,X.FECHAREQUERIDA<BR>                  ,NUMEROCTE = C.CLIENTE<BR<CONTINUA>
SQL004=<CONTINUA>>                  ,NOMBRECTE = C.NOMBRE<BR>                  ,AGENTE = V.AGENTE<BR>                  ,ALM = MAX(V.ALMACEN)<BR>                  ,CONDICION = Max(V.Condicion)<BR>                  ,FormaPago = Max(V.FormaPagoTipo)<BR>                  ,TELEFONO = Max(C.Telefonos)<BR>                  ,IMPTPEDIDO = V.PrecioTotal<BR>                  ,VD.ARTICULO<BR>                  ,A.DESCRIPCION1<BR>                  ,VD.CANTIDAD<BR>                  ,VD.PRECIO<BR><BR><BR>                                 FROM X INNER JOIN VENTA V  ON X.ORIGENID=V.MOVID  LEFT JOIN CTE C ON C.CLIENTE = V.CLIENTE<BR>                                          LEFT JOIN EQUIPOAGENTE E on V.AGENTE = E.AGENTE LEFT JOIN SUCURSAL S on S.SUCURSAL=V.SUCURSALVENTA<BR>                                            LEFT JOI<CONTINUA>
SQL005=<CONTINUA>N VENTAD VD ON V.ID=VD.ID LEFT JOIN ART A ON VD.ARTICULO=A.ARTICULO LEFT JOIN CAUSA CA ON CA.Causa = V.Causa <BR><BR>                               where 1=1 <BR>                                                   {Si(ConDatos(Info.FechaD),<T> AND X.FECHAPED BETWEEN <T>+Comillas(FechaFormatoServidor(Info.FechaD))+<T> AND <T>+COMILLAS(FechaFormatoServidor(Info.FechaA)), <T><T>)}<BR><TAB><TAB><TAB><TAB><TAB><TAB>             {Si(ConDatos(Mavi.SucursalD),<T> AND V.SUCURSALVENTA  BETWEEN <T>+Comillas(Mavi.SucursalD)+ <T> AND <T>+Comillas(Mavi.SucursalA), <T><T>)}<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB>           {SI(ConDatos(Info.Cliente), <T> AND C.CLIENTE = <T>+Comillas(Info.Cliente),<T><T>)}<BR>and v.mov like <T>PEDIDO<T> <BR><BR>                         <BR><BR><TAB><TAB><TAB><TAB><TAB><TAB><CONTINUA>
SQL006=<CONTINUA><TAB><TAB><TAB><TAB>  GROUP BY X.ORIGEN,X.ORIGENID,X.FECHAPED,V.SUCURSALVENTA,C.CLIENTE,S.NOMBRE,X.PEDIDOMOV,NUMEROPED,<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB>  V.MOV,V.MOVID,V.FECHAEMISION,C.NOMBRE,V.AGENTE,V.PrecioTotal,VD.ARTICULO,<BR><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB><TAB>  A.DESCRIPCION1,VD.CANTIDAD,VD.PRECIO,CA.CAUSA,X.FECHAREQUERIDA<BR><BR><BR>ORDER BY V.SUCURSALVENTA,C.CLIENTE,X.NUMEROPED,X.ORIGENID
